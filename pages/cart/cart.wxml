<!--pages/cart/cart.wxml-->
<view class="cart-view">
	<cart-head class="cart-head" />
	<view style="margin-top: {{navigationBarHeight}}px">
		<cart-list />
		<home-recommend />
		<home-combination />
		<block wx:if="{{shopnum.num > 0}}">
			<shopcart shopnum="{{shopnum}}" class="component-shopcart-view" />
		</block>
	</view>
</view>